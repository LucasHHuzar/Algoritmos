#include <stdio.h>

int main () {
	
	float custoFabrica, custoFinal;
	
	printf("Informe o valor de custo de fabrica do carro: ");
	scanf("%f", &custoFabrica);
	
	custoFinal = custoFabrica + custoFabrica * 0.28 + custoFabrica * 0.45;
	
	
	printf("O custo final do carro e de: %.2f", custoFinal);
	
	return 0;
}
