#include <stdio.h>
#include <locale.h>

int main(){
	
	setlocale(LC_ALL,"Portuguese");
	
	int i, vetor[5];
	
	
	for(i = 0; i < 5; i++){
		
		printf ("Informe número(%d): ", i + 1);
		scanf ("%d", &vetor[i]);
				
	}
	
	printf ("\nOs números são: \n");
	
	for(i = 4; i >= 0; i--){
		
		printf ("%d ", vetor[i]);
				
	}
	
	
	
	return 0;
}
