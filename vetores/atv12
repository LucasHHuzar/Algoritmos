#include <stdio.h>

int main(){
	
	int G[6] = {1, 2, 3, 4, 5, 6}, 
	A[10] = {1, 2, 10, 4, 20, 5, 30, 6, 40, 45}, i, j, acertos;
	
	acertos = 0;
	
	for (i = 0; i < 6; i++){
		
		for (j = 0; j < 10; j++){
			
			if(G[i] == A[j]){
				printf("Voce acertou o numero: %d\n", A[j]);
				acertos++;
				break;
			}
		}
	}
	
	printf("Acertos: %d", acertos);
	
	return 0;
}
