#include <stdio.h>
#include <locale.h>

int main(){
	
	setlocale(LC_ALL,"Portuguese");
	
	int i, num, vetor[5];
	
	for(i = 0; i < 5; i++){
		
		printf ("Informe número(%d): ", i + 1);
		scanf ("%d", &vetor[i]);
				
	}
	
	for(i = 0; i < 5; i++){
		
		if(vetor[i] % 2 == 1){
		printf ("\nOs números ímpares são: %d\n", vetor[i]);
		}
				
	}
	
	return 0;
}
